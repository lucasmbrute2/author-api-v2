{
  "name": "author-api",
  "version": "2.0.0",
  "description": "Author API",
  "main": "server.js",
  "scripts": {
    "test": "vitest run --dir src/application/modules",
    "test:watch": "vitest --dir src/application/modules",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "vitest run --dir src/application/http/controllers",
    "test:create-e2e-environment": "npm link ./tests/vitest-environment-e2e-databases",
    "test:install-e2e-environment": "npm link vitest-environment-e2e-databases",
    "pretest:e2e": "run-s test:create-e2e-environment test:install-e2e-environment",
    "start:dev": "tsx watch src/application/server.ts"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cookie-parser": "1.4.6",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-async-errors": "3.1.1",
    "ioredis": "5.3.1",
    "jsonwebtoken": "9.0.0",
    "multer": "1.4.5-lts.1",
    "reflect-metadata": "0.1.13",
    "tsyringe": "4.7.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@prisma/client": "4.11.0",
    "@rocketseat/eslint-config": "1.2.0",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.17",
    "@types/jsonwebtoken": "9.0.1",
    "@types/multer": "1.4.7",
    "@types/node": "18.15.3",
    "@vitest/coverage-c8": "0.29.3",
    "npm-run-all": "4.1.5",
    "prisma": "4.11.0",
    "supertest": "6.3.3",
    "tsx": "3.12.5",
    "typescript": "5.0.2",
    "vite-tsconfig-paths": "4.0.7",
    "vitest": "0.29.3"
  }
}
